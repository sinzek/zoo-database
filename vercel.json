{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"builds": [
		{
			"src": "main/api/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": "dist" }
		}
	],
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/main/api/index.js"
		},
		{
			"source": "/((?!api/|@|.*\\.).*)",
			"destination": "/main/index.html"
		}
	],
	"outputDirectory": "dist"
}
